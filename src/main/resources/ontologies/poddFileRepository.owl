<?xml version="1.0"?>


<!DOCTYPE rdf:RDF [
    <!ENTITY owl "http://www.w3.org/2002/07/owl#" >
    <!ENTITY fileRepository "http://purl.org/podd/ns/fileRepository#" >
    <!ENTITY xsd "http://www.w3.org/2001/XMLSchema#" >
    <!ENTITY rdfs "http://www.w3.org/2000/01/rdf-schema#" >
    <!ENTITY rdf "http://www.w3.org/1999/02/22-rdf-syntax-ns#" >
]>


<rdf:RDF xmlns="http://purl.org/podd/ns/fileRepository#"
     xml:base="http://purl.org/podd/ns/fileRepository#"
     xmlns:fileRepository="http://purl.org/podd/ns/fileRepository#"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#">
    <owl:Ontology rdf:about="http://purl.org/podd/ns/fileRepository">
        <rdfs:label>The PODD Ontology for File Repository</rdfs:label>
        <owl:versionIRI rdf:resource="http://purl.org/podd/ns/version/fileRepository/1"/>
    </owl:Ontology>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Annotation properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->



    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Datatypes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    <!-- http://purl.org/podd/ns/fileRepository#hasRepositoryConfiguration -->

    <owl:ObjectProperty rdf:about="&fileRepository;hasRepositoryConfiguration">
        <rdfs:label>has File Repository Configuration</rdfs:label>
    </owl:ObjectProperty>
    
    



    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Data properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    

    <!-- http://purl.org/podd/ns/fileRepository#hasFileRepositoryAlias -->

    <owl:DatatypeProperty rdf:about="&fileRepository;hasFileRepositoryAlias">
        <rdfs:label>has File Repository Alias</rdfs:label>
        <rdfs:domain rdf:resource="&fileRepository;FileRepository"/>
        <rdfs:range rdf:resource="&xsd;string"/>
    </owl:DatatypeProperty>




    <!-- http://purl.org/podd/ns/fileRepository#hasFileRepositoryProtocol -->

    <owl:DatatypeProperty rdf:about="&fileRepository;hasFileRepositoryProtocol">
        <rdfs:label>has File Repository Protocol</rdfs:label>
        <rdfs:domain rdf:resource="&fileRepository;FileRepository"/>
        <rdfs:range rdf:resource="&xsd;string"/>
    </owl:DatatypeProperty>



    <!-- http://purl.org/podd/ns/fileRepository#hasFileRepositoryHost -->

    <owl:DatatypeProperty rdf:about="&fileRepository;hasFileRepositoryHost">
        <rdfs:label>has File Repository Host</rdfs:label>
        <rdfs:domain rdf:resource="&fileRepository;FileRepository"/>
        <rdfs:range rdf:resource="&xsd;string"/>
    </owl:DatatypeProperty>

    
    <!-- http://purl.org/podd/ns/fileRepository#hasFileRepositoryPort -->

    <owl:DatatypeProperty rdf:about="&fileRepository;hasFileRepositoryPort">
        <rdfs:label>has File Repository Port</rdfs:label>
        <rdfs:domain rdf:resource="&fileRepository;FileRepository"/>
        <rdfs:range rdf:resource="&xsd;string"/>
    </owl:DatatypeProperty>



    <!-- http://purl.org/podd/ns/fileRepository#hasFileRepositoryFingerprint -->

    <owl:DatatypeProperty rdf:about="&fileRepository;hasFileRepositoryFingerprint">
        <rdfs:label>has File Repository Fingerprint</rdfs:label>
        <rdfs:domain rdf:resource="&fileRepository;FileRepository"/>
        <rdfs:range rdf:resource="&xsd;string"/>
    </owl:DatatypeProperty>


    <!-- http://purl.org/podd/ns/fileRepository#hasFileRepositoryUsername -->

    <owl:DatatypeProperty rdf:about="&fileRepository;hasFileRepositoryUsername">
        <rdfs:label>has File Repository Username</rdfs:label>
        <rdfs:domain rdf:resource="&fileRepository;FileRepository"/>
        <rdfs:range rdf:resource="&xsd;string"/>
    </owl:DatatypeProperty>



    <!-- http://purl.org/podd/ns/fileRepository#hasFileRepositorySecret -->

    <owl:DatatypeProperty rdf:about="&fileRepository;hasFileRepositorySecret">
        <rdfs:label>has File Repository Secret</rdfs:label>
        <rdfs:range rdf:resource="&xsd;string"/>
    </owl:DatatypeProperty>


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Classes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    

    <!-- http://purl.org/podd/ns/fileRepository#FileRepository -->

    <owl:Class rdf:about="&fileRepository;FileRepository">
        <rdfs:label>Podd File Repository</rdfs:label>
        <rdfs:comment>A PODD File Repository.</rdfs:comment>
    </owl:Class>
    


    <!-- http://purl.org/podd/ns/fileRepository#SSHFileRepository -->

    <owl:Class rdf:about="&fileRepository;SSHFileRepository">
        <rdfs:subClassOf rdf:resource="&fileRepository;FileRepository"/>
        <rdfs:label>Podd SSH File Repository</rdfs:label>
        <rdfs:comment>A PODD SSH File Repository.</rdfs:comment>
    </owl:Class>

</rdf:RDF>

